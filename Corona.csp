	.EQU KEYBOARD_STATUS, $000; READ
	.EQU KEYBOARD_DATA, $001;   WRITE
	.EQU @, $D00
LOGGED:	.CHAR '';	STORE KEYLOGGED INFO HERE
	.EQU @, $AAA
PASSWORD:	.CHAR 'get pwned';

POLL_KEYBD:	INB KEYBOARD_STATUS
	JGE POLL_KEYBD;	LT SET WHEN THERE IS NO DATA
	INB KEYBOARD_DATA
	STC LOGGED
	LDX# 0
LOOP:	LDC LOGGED;	Loads first char
	CMA# PASSWORD;	Compares that to passchar
	JEQ 
	AOC# 9
	JEQ PWNAGE
	
ERASE:	LDX# 0
	CLR+ $000
	AOC# 50
	JLT ERASE
	.END
	